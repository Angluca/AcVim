--langdef=ocen
--langmap=ocen:.oc
--language-force=ocen
--exclude=*.tags
--exclude=*.md
--exclude=*.txt
--exclude=*.py
--exclude=*.c
--exclude=tests/*
--exclude=build/*
--exclude=bootstrap/*
--exclude=docs/*
--exclude=examples/*
--exclude=meta/*
--regex-ocen=/[^\#]*typedef\s+(\w+)\s*/\1/f,type/
--regex-ocen=/[^\#]*const\s+(\w+)\s*:\s*\w\s*/\1/f,type/
--regex-ocen=/[^\#]*def\s+(\w+)\s*/\1/f,func/
--regex-ocen=/[^\#]*def\s+(\w+)\s*\<.*\(/\1/f,func/
--regex-ocen=/[^\#]*def\s+[^\(]*\w+::(\w+).*\(/\1/f,func/
--regex-ocen=/[^\#]*enum\s+(\w+)\s*/\1/f,type/
--regex-ocen=/[^\#]*enum\s+(\w+)\s*\<*.*\{/\1/f,type/
--regex-ocen=/[^\#]*enum\s+[^\(]*::(\w+).*\{/\1/f,type/
--regex-ocen=/[^\#]*struct\s+(\w+)\s*/\1/f,type/
--regex-ocen=/[^\#]*struct\s+(\w+)\s*\<*.*\{/\1/f,type/
--regex-ocen=/[^\#]*struct\s+[^\(]*::(\w+)[^:]*\{/\1/f,type/
